<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnPrevious"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:freezesText="true"
        android:text="进入ThreeFragment"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.238" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="58dp"
        android:text="This is SecondFragment 2"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/btnPrevious"
        app:layout_constraintEnd_toEndOf="@+id/btnPrevious"
        app:layout_constraintStart_toStartOf="@+id/btnPrevious" />

    <Button
        android:id="@+id/btnHasParamAndResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="有参数有返回值"
        app:layout_constraintEnd_toEndOf="@+id/btnPrevious"
        app:layout_constraintHorizontal_bias="0.607"
        app:layout_constraintStart_toStartOf="@+id/btnPrevious"
        app:layout_constraintTop_toBottomOf="@+id/view_textview" />

    <Button
        android:id="@+id/btnHasParamNoResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="有参数无返回值"
        app:layout_constraintEnd_toEndOf="@+id/btnHasParamAndResult"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnHasParamAndResult"
        app:layout_constraintTop_toBottomOf="@+id/btnHasParamAndResult" />

    <Button
        android:id="@+id/btnNoParamHasResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="无参数有返回值"
        app:layout_constraintEnd_toEndOf="@+id/btnHasParamNoResult"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnHasParamNoResult"
        app:layout_constraintTop_toBottomOf="@+id/btnHasParamNoResult" />

    <Button
        android:id="@+id/btnNoParamNoResult"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:text="无参数无返回值"
        app:layout_constraintEnd_toEndOf="@+id/btnNoParamHasResult"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnNoParamHasResult"
        app:layout_constraintTop_toBottomOf="@+id/btnNoParamHasResult" />

    <TextView
        android:id="@+id/view_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="Fragment和Activity之间通信"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="@+id/btnPrevious"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="@+id/btnPrevious"
        app:layout_constraintTop_toBottomOf="@+id/btnPrevious" />

</androidx.constraintlayout.widget.ConstraintLayout>
