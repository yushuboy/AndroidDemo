<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.axaet.module.view">

    <application
        android:name=".application.DebugApplicationImpl"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/view_app_name"
        android:supportsRtl="true"
        android:theme="@style/view_AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:label="MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ComplexRecyclerViewActivity"
            android:label="RecycleView复杂布局" />
        <activity
            android:name=".activity.FragmentStackActivity"
            android:label="单Activity+多Fragment" />
    </application>

</manifest>